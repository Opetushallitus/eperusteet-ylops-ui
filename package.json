{
  "name": "eperusteet-ylops-lukio-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dcci": "docker-compose build && docker-compose up --abort-on-container-exit",
    "dctests": "npm run install_cypress && npm run test:e2e",
    "dcci:serve": "vue-cli-service serve",
    "dcci:dev": "docker-compose build && docker-compose up db",
    "install_cypress": "npx cypress install && npx cypress verify",
    "test": "npm run test:unit && npm run test:e2e",
    "lint": "vue-cli-service lint",
    "eslint": "eslint src/**/*.{ts,vue}",
    "ci:start": "npm run lint && npm run gen:api && npm run build && npm run test",
    "clean": "npx jest --clearCache",
    "format": "vue-cli-service lint",
    "gen:api": "./scripts/buildapi.sh",
    "stats": "npm test && $BROWSER mochawesome-report/mochawesome.html coverage/lcov-report/index.html",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:e2e:dev": "vue-cli-service test:e2e",
    "test:unit": "TZ=Europe/Helsinki vue-cli-service test:unit --ci --coverage --no-cache",
    "test:unit:dev": "TZ=Europe/Helsinki vue-cli-service test:unit --watch --no-cache",
    "test:unit:report": "$BROWSER coverage/lcov-report/index.html"
  },
